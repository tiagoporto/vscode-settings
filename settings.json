{
  "[sass]": {
    "editor.defaultFormatter": "syler.sass-indented"
  },
  "[stylus]": {
    "editor.defaultFormatter": "thisismanta.stylus-supremacy"
  },
  "clipboard-manager.maxClips": 20,
  "clipboard-manager.preview": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Hasklig, 'Fira Code'",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80],
  "eslint.alwaysShowStatus": true,
  "explorer.compactFolders": false,
  "files.associations": {
    "fastcgi.conf": "NGINX",
    "mime.types": "NGINX",
    "nginx.conf": "NGINX",
    "proxy.conf": "NGINX"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.cache-loader": true,
    "**/build": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true
  },
  "files.trimTrailingWhitespace": true,
  "indentRainbow.colors": [
    "rgba(255,255,64,0.15)",
    "rgba(127,255,127,0.2)",
    "rgba(255,127,255,0.15)",
    "rgba(0, 35, 255,0.2)"
  ],
  "indentRainbow.errorColor": "rgba(128,32,32,0.6)",
  "indentRainbow.tabmixColor": "rgba(128,32,96,0.6)",
  "search.exclude": {},
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    "TODO:",
    {
      "backgroundColor": "lightpink",
      "color": "red",
      "text": "BUG:"
    },
    {
      "backgroundColor": "aqua",
      "color": "navy",
      "text": "NOTE:"
    },
    "HACK:"
  ],
  "workbench.colorTheme": "Noctis Obscuro",
  "workbench.iconTheme": "material-icon-theme",
  "editor.foldingStrategy": "indentation",
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "redhat.telemetry.enabled": false
}
